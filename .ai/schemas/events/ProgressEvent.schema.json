{  "$schema": "http://json-schema.org/draft-07/schema#",  "title": "ProgressEvent",  "type": "object",  "required": ["job_id", "phase", "status", "timestamp"],  "properties": {    "job_id": { "type": "string" },    "workstream_id": { "type": ["string","null"] },    "phase": { "type": "string", "enum": ["intake","plan","dispatch","execute","aggregate","verify","ship","done"] },    "status": { "type": "string", "enum": ["queued","running","needs-info","blocked","retrying","failed","succeeded","canceled"] },    "status_reason": { "type": ["string","null"] },    "percent": { "type": "number", "minimum": 0, "maximum": 100 },    "message": { "type": "string" },    "result_uri": { "type": "string" },    "timestamp": { "type": "string", "format": "date-time" },    "metrics": {      "type": "object",      "properties": {        "duration_ms": { "type": "integer", "minimum": 0 },        "cost_usd": { "type": "number", "minimum": 0 },        "remaining_budget": { "type": "number", "minimum": 0 },        "tokens_in": { "type": "integer", "minimum": 0 },        "tokens_out": { "type": "integer", "minimum": 0 }      },      "additionalProperties": true    }  },  "additionalProperties": true}