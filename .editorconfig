# EditorConfig is awesome: https://EditorConfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.{yml,yaml,json,md,ts,tsx}]
indent_size = 2

[*.{bat,cmd,ps1}]
end_of_line = crlf
indent_style = space
indent_size = 2
