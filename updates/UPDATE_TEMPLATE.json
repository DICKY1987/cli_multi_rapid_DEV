{
  "$schema": ".ai/schemas/plan_update.schema.json",
  "update_id": "YYYY-MM-DDTHHMM_project_slug",
  "version": "1.0.0",
  "target_repo": "DICKY1987/cli_multi_rapid_DEV",
  "metadata": {
    "author": "AI/agent",
    "created": "YYYY-MM-DD",
    "description": "Short description of the change set"
  },
  "deliverables": [
    {
      "id": "doc.plan_updates",
      "type": "file",
      "title": "Plan Updates Guide",
      "path": "docs/plan-updates.md",
      "must_exist": true,
      "must_contain": ["Plan Updates Workflow"]
    }
  ],
  "acceptance_criteria": [
    {
      "type": "assert_contains",
      "file": "docs/plan-updates.md",
      "must_contain": ["Plan Updates Workflow"]
    }
  ],
  "operations": [
    {
      "type": "upsert_ops",
      "module": "40-files-docs.json",
